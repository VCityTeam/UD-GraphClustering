# return a workspace graph (spaces, scenarios, versionTransitions, and versions) and their immediate properties

SELECT DISTINCT ?subject ?subjectType ?predicate ?object ?objectType
WHERE {
  {
    ?subject a wksp:Workspace ;
      ?predicate ?object .
    ?subject a ?subjectType .
    OPTIONAL {
      ?object a ?objectType .
  	  FILTER(?objectType != owl:NamedIndividual)
    }
  } UNION {
    ?subject a wksp:ConcensusSpace ;
      ?predicate ?object .
    ?subject a ?subjectType .
    OPTIONAL {
      ?object a ?objectType .
  	  FILTER(?objectType != owl:NamedIndividual)
    }
  } UNION {
    ?subject a wksp:PropositionSpace ;
      ?predicate ?object .
    ?subject a ?subjectType .
    OPTIONAL {
      ?object a ?objectType .
  	  FILTER(?objectType != owl:NamedIndividual)
    }
  } UNION {
    ?subject a wksp:Scenario ;
      ?predicate ?object .
    ?subject a ?subjectType .
    OPTIONAL {
      ?object a ?objectType .
  	  FILTER(?objectType != owl:NamedIndividual)
    }
  } UNION {
    ?subject a vers:Version ;
      ?predicate ?object .
    ?subject a ?subjectType .
    OPTIONAL {
      ?object a ?objectType .
  	  FILTER(?objectType != owl:NamedIndividual)
    }
  } UNION {
    ?subject a vers:VersionTransition ;
      ?predicate ?object .
    ?subject a ?subjectType .
    OPTIONAL {
      ?object a ?objectType .
  	  FILTER(?objectType != owl:NamedIndividual)
    }
  }
  FILTER(?predicate != rdf:type)
  FILTER(?predicate != vers:Version.versionMember)
  FILTER(?predicate != vers:VersionTransition.transaction)
  FILTER(?subject != owl:NamedIndividual)
  FILTER(?object != owl:NamedIndividual)
  FILTER(?subjectType != owl:NamedIndividual)
}